{
  "expo": {
    "name": "SAD LAS Worker",
    "slug": "sad-las-worker",
    "scheme": "sadlasworker",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["android"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b82f6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sadlas.worker"
    },
    "android": {
      "package": "com.sadlas.worker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#3b82f6"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "plugins": ["expo-notifications"],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#3b82f6"
    },
    "extra": {
      "eas": {
        "projectId": "12345678-1234-1234-1234-123456789012"
      }
    },
    "build": {
      "android": {
        "buildType": "apk"
      }
    }
  }
}
